{"ast":null,"code":"export default {\n  name: 'App',\n  data() {\n    return {\n      trialCount: 1,\n      hints: 3,\n      word: \"hellooo\",\n      idTrial: 10\n    };\n  },\n  mounted() {\n    window.addEventListener('keypress', this.assignLetters);\n    this.newTrial();\n    this.this.word.toUpperCase;\n    console.log(this.word);\n  },\n  methods: {\n    newTrial() {\n      let trialText = \"\";\n      trialText += `<div id=\"${this.idTrial}\">\n              <div class=\"try\">Try ${this.idTrial - 9}</div>`;\n      for (let i = 0; i < this.word.length; i++) {\n        trialText += `<div class=\"input\" id=\"${i}\"></div>`;\n      }\n      trialText += `</div>`;\n      document.getElementById(\"trials\").innerHTML += trialText;\n    },\n    useHint() {\n      if (this.hints > 0) {\n        //get random no. instead\n        let place = document.getElementById(String(this.idTrial)).childNodes[0 + 2];\n        place.innerHTML = this.word[0];\n        place.style.backgroundColor = \"#ff9a09\";\n        place.style.color = \"#ffffff\";\n        place.style.border = \"none\";\n        place.style.opacity = \"0.5\";\n        this.hints--;\n      }\n      if (this.hints == 0) this.disableHints();\n    },\n    disableHints() {\n      document.getElementById(\"hint\").style.backgroundColor = \"#016264\";\n      document.getElementById(\"hint\").style.color = \"#\";\n    },\n    assignLetters(event) {\n      if (event === undefined) {\n        console.log(\"bye\");\n      } else {\n        if (event.keyCode == 13) {\n          this.checkWord();\n        }\n      }\n    },\n    checkWord() {\n      console.log(\"hiiiii\");\n      let wrongFlag = 0;\n      for (let i = 0; i < this.word.length; i++) {\n        console.log(this.word[i]);\n        console.log(document.getElementById(String(this.idTrial)).childNodes[i + 2].innerHTML);\n        if (document.getElementById(String(i)).innerHTML == this.word[i]) {} else {\n          wrongFlag = 1;\n        }\n      }\n      if (wrongFlag) {\n        this.idTrial += 1;\n        this.newTrial();\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","trialCount","hints","word","idTrial","mounted","window","addEventListener","assignLetters","newTrial","this","toUpperCase","console","log","methods","trialText","i","length","document","getElementById","innerHTML","useHint","place","String","childNodes","style","backgroundColor","color","border","opacity","disableHints","event","undefined","keyCode","checkWord","wrongFlag"],"sources":["E:\\FrontEnd Projects\\guess-the-word\\src\\App.vue"],"sourcesContent":["<template>\n  <body>\n    <div class=\"main-app\">\n      <header>Guess The Word</header>\n      <div class=\"main-container\">\n        <div class=\"secondary-container\">\n          <section class=\"trials\" id=\"trials\">\n          </section>\n\n          <footer>\n            <button class=\"check-word\" @click=\"checkWord()\">Check Word</button>\n            <button class=\"hint\" id=\"hint\" @click=\"useHint\">{{ hints }} Hint</button>\n          </footer>\n        </div>\n\n        <section class=\"color-hints\">\n          <div class=\"no-style\">Key Colors</div>\n          <div class=\"right-in-place\">Letter is Right And in Place</div>\n          <div class=\"right-not-in-place\">Letter is Right But Not in Place</div>\n          <div class=\"wrong\">Letter is Wrong And Not Exists in Word</div>\n        </section>\n      </div>\n\n    </div>\n  </body>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  data() {\n    return {\n      trialCount : 1,\n      hints : 3,\n      word : \"hellooo\",\n      idTrial : 10\n    }\n  },\n  mounted(){\n    window.addEventListener('keypress', this.assignLetters)\n    this.newTrial()\n    this.this.word.toUpperCase\n    console.log(this.word)\n  },\n  methods:{\n    newTrial(){\n      let trialText = \"\"\n      trialText += `<div id=\"${this.idTrial}\">\n              <div class=\"try\">Try ${this.idTrial-9}</div>`\n      for(let i = 0 ; i < this.word.length ; i++){\n        trialText += `<div class=\"input\" id=\"${i}\"></div>`\n      }\n      trialText += `</div>`\n      document.getElementById(\"trials\").innerHTML += trialText\n    },\n    useHint(){\n      if(this.hints > 0){\n        //get random no. instead\n        let place = document.getElementById(String(this.idTrial)).childNodes[0+2]\n        place.innerHTML = this.word[0]\n        place.style.backgroundColor = \"#ff9a09\"\n        place.style.color = \"#ffffff\"\n        place.style.border = \"none\"\n        place.style.opacity = \"0.5\"\n        \n        this.hints --\n      }\n        \n      if(this.hints == 0)\n        this.disableHints()\n\n    },\n    disableHints(){\n      document.getElementById(\"hint\").style.backgroundColor = \"#016264\"\n      document.getElementById(\"hint\").style.color = \"#\"\n    },\n    assignLetters(event){\n      if(event === undefined){\n        console.log(\"bye\")\n      } else {\n        if(event.keyCode == 13){\n          this.checkWord()\n        }\n      }\n    },\n    checkWord(){\n      console.log(\"hiiiii\")\n      let wrongFlag = 0\n      for(let i = 0 ;i < this.word.length; i++){\n        console.log(this.word[i])\n        console.log(document.getElementById(String(this.idTrial)).childNodes[i+2].innerHTML)\n        if(document.getElementById(String(i)).innerHTML == this.word[i]){\n          \n        } else {\n          wrongFlag = 1\n        }\n      }\n      if(wrongFlag){\n        this.idTrial += 1\n        this.newTrial()\n      }\n        \n    }\n  }\n}\n</script>\n\n<style>\nhtml,body {\n  margin-top: 70px;\n  background-color: #1a202a;\n}\n.main-app {\n  padding: 10px;\n  background-color: #ebebeb;\n  margin: auto;\n  max-width: 800px;\n  min-height: 400px;\n  display: flex;\n  flex-direction: column;\n  position: relative;\n}\n* {\n  font-family: 'Cairo', sans-serif;\n  text-align: center;\n  font-weight: bold;\n}\nheader {\n  background-color: #ffffff;\n  color: 